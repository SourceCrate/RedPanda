
  "keymaps": {
    "0": {
      "type": "scroll.home"
    },
    ":": {
      "type": "command.show"
    },
    "o": {
      "type": "command.show.open",
      "alter": false
    },
    "O": {
      "type": "command.show.open",
      "alter": true
    },
    "t": {
      "type": "command.show.tabopen",
      "alter": false
    },
    "T": {
      "type": "command.show.tabopen",
      "alter": true
    },
    "w": {
      "type": "command.show.winopen",
      "alter": false
    },
    "W": {
      "type": "command.show.winopen",
      "alter": true
    },
    "b": {
      "type": "command.show.buffer"
    },
    "a": {
      "type": "command.show.addbookmark",
      "alter": true
    },
    "k": {
      "type": "scroll.vertically",
      "count": -1
    },
    "j": {
      "type": "scroll.vertically",
      "count": 1
    },
    "h": {
      "type": "scroll.horizonally",
      "count": -1
    },
    "l": {
      "type": "scroll.horizonally",
      "count": 1
    },
    "u": {
      "type": "scroll.pages",
      "count": -0.5
    },
    "i": {
      "type": "scroll.pages",
      "count": 0.5
    },
    "": {
      "type": "page.home",
      "newTab": true
    },
    "gg": {
      "type": "scroll.top"
    },
    "G": {
      "type": "scroll.bottom"
    },
    "$": {
      "type": "scroll.end"
    },
    "q": {
      "type": "tabs.close",
      "select": "right"
    },
    "U": {
      "type": "tabs.reopen"
    },
    "J": {
      "type": "tabs.prev"
    },
    "K": {
      "type": "tabs.next"
    },
    "r": {
      "type": "tabs.reload",
      "cache": false
    },
    "R": {
      "type": "tabs.reload",
      "cache": true
    },
    "+": {
      "type": "zoom.in"
    },
    "-": {
      "type": "zoom.out"
    },
    "f": {
      "type": "follow.start",
      "newTab": false,
      "background": false
    },
    "F": {
      "type": "follow.start",
      "newTab": true,
      "background": false
    },
    "H": {
      "type": "navigate.history.prev"
    },
    "L": {
      "type": "navigate.history.next"
    },
    "y": {
      "type": "urls.yank"
    },
    "p": {
      "type": "urls.paste",
      "newTab": false
    },
    "P": {
      "type": "urls.paste",
      "newTab": true
    },
    "/": {
      "type": "find.start"
    },
    "n": {
      "type": "find.next"
    },
    "N": {
      "type": "find.prev"
    },
    ".": {
      "type": "repeat.last"
    },
    "<S-Esc>": {
      "type": "addon.toggle.enabled"
    }
  },
  "search": {
    "default": "Qwant",
    "engines": {
      "Qwant": "https://qwant.com/?q={}"
    }
  },
  "properties": {
    "hintchars": "abcdefghijklmnopqrstuvwxyz",
    "smoothscroll": false,
    "complete": "sbh"
  },
  "blacklist": []
}
